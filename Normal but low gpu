-- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local TextLabel = Instance.new("TextLabel")
local UICorner_2 = Instance.new("UICorner")
local TextLabel_2 = Instance.new("TextLabel")
local TextButton = Instance.new("TextButton")
local UICorner_3 = Instance.new("UICorner")

--Properties:

ScreenGui.Parent = game:GetService("CoreGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.AnchorPoint = Vector2.new(0.5, 0.5) -- ทำให้ Frame อยู่ตรงกลางจอ
Frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
Frame.BackgroundTransparency = 0.250
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.ClipsDescendants = true
Frame.Position = UDim2.new(0.5, 0, 0.5, 0) -- ปรับ Position ให้ตรงกลาง
Frame.Size = UDim2.new(0, 250, 0, 180) -- ลดขนาด Frame

UICorner.Parent = Frame

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
TextLabel.BackgroundTransparency = 1.000
TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.BorderSizePixel = 0
TextLabel.Size = UDim2.new(0, 250, 0, 30) -- ลดขนาด TextLabel
TextLabel.Font = Enum.Font.FredokaOne
TextLabel.Text = "Disclaimer"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextSize = 30.000 -- ลดขนาดข้อความ
TextLabel.TextWrapped = true

UICorner_2.Parent = TextLabel

TextLabel_2.Parent = Frame
TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel_2.BackgroundTransparency = 1.000
TextLabel_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextLabel_2.Position = UDim2.new(0.1, 0, 0.15, 0)
TextLabel_2.Size = UDim2.new(0, 230, 0, 70) -- ลดขนาดข้อความ
TextLabel_2.Font = Enum.Font.FredokaOne
TextLabel_2.Text = "Use script at your own risk. I am not responsible for any accounts banned using my script."
TextLabel_2.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel_2.TextSize = 16.000 -- ลดขนาดข้อความ
TextLabel_2.TextWrapped = true
TextLabel_2.TextXAlignment = Enum.TextXAlignment.Left

TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
TextButton.BackgroundTransparency = 0.550
TextButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextButton.BorderSizePixel = 0
TextButton.Position = UDim2.new(0.25, 0, 0.75, 0)
TextButton.Size = UDim2.new(0, 80, 0, 30) -- ลดขนาดปุ่ม
TextButton.Font = Enum.Font.FredokaOne
TextButton.Text = "Okay"
TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
TextButton.TextSize = 18.000 -- ลดขนาดข้อความในปุ่ม

UICorner_3.Parent = TextButton

-- Variables
local webhookURL = "YOUR_WEBHOOK_URL_HERE" -- เปลี่ยนเป็น URL ของ webhook ของคุณ

-- Functions

local function getMoneyAndTrophies()
    local player = game.Players.LocalPlayer
    local money = player:FindFirstChild("Money") and player.Money.Value or 0 -- ดึงจำนวนเงิน
    local trophies = player:FindFirstChild("Trophies") and player.Trophies.Value or 0 -- ดึงจำนวนถ้วยรางวัล
    return money, trophies
end

local function sendWebhookNotification()
    local HttpService = game:GetService("HttpService")
    local money, trophies = getMoneyAndTrophies() -- เรียกดูจำนวนเงินและถ้วยรางวัล
    local data = {
        content = string.format("Script is running smoothly.\nCurrent Money: %d\nCurrent Trophies: %d", money, trophies)
    }
    local jsonData = HttpService:JSONEncode(data)

    local success, response = pcall(function()
        return HttpService:PostAsync(webhookURL, jsonData, Enum.HttpContentType.ApplicationJson)
    end)

    if success then
        print("Webhook notification sent successfully.")
    else
        warn("Error sending webhook notification: " .. response)
    end
end

local function BoostFPS() -- เพิ่มฟังก์ชัน Boost FPS
    -- ลดการแสดงผลที่ไม่จำเป็นเพื่อเพิ่ม FPS
    settings().Rendering.QualityLevel = Enum.QualityLevel.Level01 -- ตั้งค่าให้กราฟิกต่ำสุด
    settings().Network.IncomingReplicationLag = 100 -- เพิ่มความล่าช้าในการรับส่งข้อมูลเพื่อประหยัดทรัพยากร

    for _, child in pairs(game:GetDescendants()) do
        if child:IsA("BasePart") then
            child.Material = Enum.Material.SmoothPlastic -- เปลี่ยนวัสดุเป็น SmoothPlastic
            child.CastShadow = false -- ปิดการแสดงผลเงา
        end
        if child:IsA("Decal") or child:IsA("Texture") then
            child:Destroy() -- ลบเอฟเฟกต์ภาพที่ไม่จำเป็น
        end
        if child:IsA("ParticleEmitter") or child:IsA("Light") then
            child.Enabled = false -- ปิด Particle และ Light
        end
    end
    print("FPS Boosted!")
end

local function WSDES_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local button = script.Parent
	local frame = button.Parent
	button.MouseButton1Down:Connect(function()
		-- เคลื่อนย้าย Frame ออกไปจากหน้าจอ
		frame:TweenPosition(UDim2.new(1,0,1,0),Enum.EasingDirection.InOut,Enum.EasingStyle.Quart,0.5) -- เปลี่ยนระยะเวลาในการเคลื่อนที่ให้สั้นลง
		task.wait(0.5)
		frame.Parent:Destroy()

		-- ตรวจสอบการโหลด URL ของสคริปต์ก่อนเรียกใช้
		local success, result = pcall(function()
			return loadstring(game:HttpGet("https://raw.githubusercontent.com/NinjaDragonVip/Taxitest/refs/heads/main/Normal%20but%20low%20gpu"))()
		end)
		if success then
			print("Script loaded successfully.")
		else
			warn("Error loading script: "..tostring(result)) -- แสดงผลเมื่อมีข้อผิดพลาด
		end
	end)
end

coroutine.wrap(WSDES_fake_script)()

-- เรียกใช้ฟังก์ชัน BoostFPS อัตโนมัติเมื่อสคริปต์ทำงาน
BoostFPS()

-- ฟังก์ชันสำหรับการแจ้งเตือนทุก ๆ 10 นาที
while true do
    task.wait(600) -- รอ 10 นาที (600 วินาที)
    sendWebhookNotification() -- ส่งการแจ้งเตือน
end
