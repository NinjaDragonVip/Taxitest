-- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local TextLabel = Instance.new("TextLabel")
local UICorner_2 = Instance.new("UICorner")
local TextLabel_2 = Instance.new("TextLabel")
local AutoBuyButton = Instance.new("TextButton") -- ปุ่มสำหรับ Auto Buy All Car
local UICorner_3 = Instance.new("UICorner")
local AutoBuyState = false -- สถานะเปิด/ปิด Auto Buy

--Properties:

ScreenGui.Parent = game:GetService("CoreGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.AnchorPoint = Vector2.new(0.5, 0.5) -- ทำให้ Frame อยู่ตรงกลางจอ
Frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
Frame.BackgroundTransparency = 0.250
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.ClipsDescendants = true
Frame.Position = UDim2.new(0.5, 0, 0.5, 0) -- ปรับ Position ให้ตรงกลาง
Frame.Size = UDim2.new(0, 400, 0, 259)

UICorner.Parent = Frame

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
TextLabel.BackgroundTransparency = 1.000
TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.BorderSizePixel = 0
TextLabel.Size = UDim2.new(0, 400, 0, 50)
TextLabel.Font = Enum.Font.FredokaOne
TextLabel.Text = "Auto Buy All Car"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextSize = 43.000
TextLabel.TextWrapped = true

UICorner_2.Parent = TextLabel

TextLabel_2.Parent = Frame
TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel_2.BackgroundTransparency = 1.000
TextLabel_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextLabel_2.Position = UDim2.new(0.147499993, 0, 0.250965238, 0)
TextLabel_2.Size = UDim2.new(0, 282, 0, 109)
TextLabel_2.Font = Enum.Font.FredokaOne
TextLabel_2.Text = "Toggle Auto Buy All Car"
TextLabel_2.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel_2.TextSize = 20.000
TextLabel_2.TextWrapped = true
TextLabel_2.TextXAlignment = Enum.TextXAlignment.Left

AutoBuyButton.Parent = Frame
AutoBuyButton.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
AutoBuyButton.BackgroundTransparency = 0.550
AutoBuyButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
AutoBuyButton.BorderSizePixel = 0
AutoBuyButton.Position = UDim2.new(0.282499999, 0, 0.745173752, 0)
AutoBuyButton.Size = UDim2.new(0, 157, 0, 43)
AutoBuyButton.Font = Enum.Font.FredokaOne
AutoBuyButton.Text = "OFF"
AutoBuyButton.TextColor3 = Color3.fromRGB(255, 255, 255)
AutoBuyButton.TextSize = 22.000

UICorner_3.Parent = AutoBuyButton

-- Scripts:

local function AutoBuyAllCar() -- ฟังก์ชันสำหรับซื้อรถทั้งหมด
    if AutoBuyState then
        -- เรียกใช้สคริปต์การซื้อรถ
        local success, result = pcall(function()
            return loadstring(game:HttpGet("https://raw.githubusercontent.com/doughkingofdarkness/taxiboss/refs/heads/main/loader.lua"))()
        end)
        if success then
            print("Auto Buy All Car script executed successfully.")
        else
            warn("Error in Auto Buy All Car script: "..tostring(result))
        end
    else
        print("Auto Buy All Car is OFF")
    end
end

local function WSDES_fake_script() -- ฟังก์ชันสคริปต์หลัก
	local script = Instance.new('LocalScript', AutoBuyButton)

	local button = script.Parent
	local frame = button.Parent
	
	button.MouseButton1Down:Connect(function()
		-- เปลี่ยนสถานะ AutoBuyState เมื่อกดปุ่ม
		AutoBuyState = not AutoBuyState
		if AutoBuyState then
			button.Text = "ON"
			AutoBuyAllCar() -- เรียกใช้ฟังก์ชันซื้อรถ
		else
			button.Text = "OFF"
		end
	end)
	
	-- เรียกใช้สคริปต์ Taxiboss เมื่อทำงาน
	local success, result = pcall(function()
		return loadstring(game:HttpGet("https://raw.githubusercontent.com/NinjaDragonVip/Taxitest/refs/heads/main/Taxiboss"))()
	end)
	if success then
		print("Taxiboss script loaded successfully.")
	else
		warn("Error loading Taxiboss script: "..tostring(result))
	end
end

coroutine.wrap(WSDES_fake_script)()
