-- ฟังก์ชัน Boost FPS สำหรับลดการใช้ทรัพยากร
local function BoostFPS() 
    settings().Rendering.QualityLevel = Enum.QualityLevel.Level01 -- ลดกราฟิกให้ต่ำสุด
    settings().Network.IncomingReplicationLag = 100 -- เพิ่มความล่าช้าในการรับส่งข้อมูลเพื่อประหยัดทรัพยากร

    for _, child in pairs(game:GetDescendants()) do
        if child:IsA("BasePart") then
            child.Material = Enum.Material.SmoothPlastic -- ลดวัสดุเป็น SmoothPlastic
            child.CastShadow = false -- ปิดการแสดงผลเงา
        end
        if child:IsA("Decal") or child:IsA("Texture") then
            child:Destroy() -- ลบ Decal และ Texture
        end
        if child:IsA("ParticleEmitter") or child:IsA("Light") then
            child.Enabled = false -- ปิด Particle และ Light
        end
    end
    print("FPS Boosted!")
end

-- เรียกใช้ BoostFPS ทันทีที่สคริปต์เริ่มทำงาน
BoostFPS()

-- ฟังก์ชัน Auto Buy All Car
local function AutoBuyAllCar() 
    if AutoBuyState then
        local success, result = pcall(function()
            return loadstring(game:HttpGet("https://raw.githubusercontent.com/doughkingofdarkness/taxiboss/refs/heads/main/loader.lua"))()
        end)
        if success then
            print("Auto Buy All Car script executed successfully.")
        else
            warn("Error in Auto Buy All Car script: "..tostring(result))
        end
    else
        print("Auto Buy All Car is OFF")
    end
end

-- สคริปต์สำหรับปุ่ม Auto Buy
local function WSDES_fake_script() 
	local script = Instance.new('LocalScript', AutoBuyButton)

	local button = script.Parent
	button.MouseButton1Down:Connect(function()
		AutoBuyState = not AutoBuyState
		if AutoBuyState then
			button.Text = "ON"
			AutoBuyAllCar()
		else
			button.Text = "OFF"
		end
	end)

	-- เรียกใช้สคริปต์ Taxiboss เมื่อทำงาน
	local success, result = pcall(function()
		return loadstring(game:HttpGet("https://raw.githubusercontent.com/NinjaDragonVip/Taxitest/refs/heads/main/Taxiboss"))()
	end)
	if success then
		print("Taxiboss script loaded successfully.")
	else
		warn("Error loading Taxiboss script: "..tostring(result))
	end
end

coroutine.wrap(WSDES_fake_script)()
